{
	"name": "TED Environment Switcher",
	"version": "0.3",
	"description": "Go where you want",
	"permissions": ["activeTab", "declarativeContent", "storage"],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images/ted-16.png",
			"32": "images/ted-32.png",
			"48": "images/ted-48.png",
			"128": "images/ted-128.png"
		}
	},
	"icons": {
		"16": "images/ted-16.png",
		"32": "images/ted-32.png",
		"48": "images/ted-48.png",
		"128": "images/ted-128.png"
	},
	"manifest_version": 2
}